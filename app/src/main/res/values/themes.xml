<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.StudentList2022" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/acu_purple</item>
        <item name="colorPrimaryVariant">@color/acu_gray</item>
        <item name="colorOnPrimary">@color/acu_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ProfTannerCustomTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/rwt_customBlue</item>
        <item name="colorPrimaryDark">@color/rwt_customBlueDark</item>
        <item name="colorAccent">@color/rwt_customRed</item>
    </style>

    <style name="AlissaDavisCustomTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/acd_mediumGreen</item>
        <item name="colorPrimaryDark">@color/acd_forestGreen</item>
        <item name="colorAccent">@color/acd_lightChittimWood</item>
    </style>

    <style name="AnthonyMoncivaisCustomTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/am_dark_green</item>
        <item name="colorSecondary">@color/am_dark_blue</item>
        <item name="colorAccent">@color/am_neon_green</item>
        <item name="android:colorBackground">@color/am_dark_blue</item>
    </style>

</resources>